config_version=5

[application]

config/name="NZG Software Launcher"
config/description="Programme to launch and manage all other NZG products."
config/version="1.0.0"
run/main_scene="uid://bbj6gwox8hrf3"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=30
run/low_processor_mode=true
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 0)
config/icon="uid://d3hmw36usl53l"
config/windows_native_icon="res://Assets/NSL_Icon.ico"

[autoload]

IconLoader="*res://Scripts/Globals/IconLoader.gd"
ColourManager="*res://Scripts/Globals/ColourManager.gd"
UserManager="*res://Scripts/Globals/UserManager.gd"
GeneralManager="*res://Scripts/Globals/GeneralManager.gd"
UpdateManager="*res://Scripts/Globals/UpdateManager.gd"
APIManager="*res://Scripts/Globals/APIManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/size/borderless=true
window/size/window_width_override=1440
window/size/window_height_override=810
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/NatCustomExporter/plugin.cfg")

[global_group]

Separator="Separator"
"Background 2"="Secondary Background"
"Background 1"="Background"
"Stylebox Haver"="Has A Stylebox"
Shaded="Shaded"

[gui]

theme/custom_font="uid://dl5fa4m7mvrve"
theme/default_font_antialiasing=2

